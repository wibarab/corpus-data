    <TIER ANNOTATOR="Rebecca" LINGUISTIC_TYPE_REF="Phrases "
        PARTICIPANT="SAU_2022_Speaker10" TIER_ID="SAU_2022_Speaker10_Transcription-txt">
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1"
                TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE>ē maṯalan al-ḥīn tzawwaǧat aḥda banāt-i aw aḥda xawāt-i aw aḥda yaʕni qarbāʔ-i, ē b-ingūl-ah ((why -ah for a woman?)) alf mabrūk w-aḷḷa yusād-ik (yusāʕid-ik) duʕa ṭabʕan at-tawfīq</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2"
                TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE>w-nwussī-ha ʕala mā-gāl rasūl ṣalla aḷḷah ʕaley-h wa-sallam</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3"
                TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE>ē kūni-l-ah ā umm-an yikūn-li-ki ʕabd-an </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4"
                TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>ē wa-aḷḷah w-ṣalla ʕala muḥammad w min kill māl wa-min-h il-māl wa-min-ki il-ʕiyāl inšāʔaḷḷa
</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5"
                TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>fahamti?</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6"
                TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE>yaʕni, maṯalan hal-ḥīn  tizawwaǧat aḥda banāt-i fā wassī-ha yaʕni wassiyyāt, kēf tkūn ila-h zay al-umm yaʕni tkunīn anti w-umm-a yaʕni, ḥanūna ʕalē-h ṭayba maʕā-h, rafīqa maʕā-_́ b-kill  šay tḥāfḏ̣īn ʕaley-h miṯil-mā tḥāfaḏ̣  ʕaley-h umm-ah yakūn li-ki  ʕabd-an          </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7"
                TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts17">
                <ANNOTATION_VALUE>yaʕni ykūn li-ki zay mā ytīʕ umm-a yṣīr li-ki zay-ha, yaʕni ʕabd maṯalan, fahamti?</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8"
                TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE>aywa wa-min-hu, hū l-māl  hū min-h al-māl li yidfaʕ it-takālif iz-zawāǧ wa-l-maṣārīf, wa-hī min-ha l-ʕiyāl min-ha l-ʕiyāl, yitxilif li-ha l-awlād wa-l-banāt
</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9"
                TIME_SLOT_REF1="ts22" TIME_SLOT_REF2="ts25">
                <ANNOTATION_VALUE> fahamti? </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10"
                TIME_SLOT_REF1="ts26" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE>w hāḏa ṭabʕan fī s-saʔīl yaʕni l-muslimīn  ʕandana yaʕni muš ((muš!!!)) b-ir-rašāyda xāṣatan</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
